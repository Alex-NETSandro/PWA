@using PWABlog.ViewModels.Admin.etiqueta
@model PWABlog.ViewModels.Admin.etiqueta.EtiquetaDetailsViewModel
@{
    ViewBag.Title = @ViewBag.title;
    Layout = "_Layout";
}

<div class="form-group">
    @Html.Label(null,"Id",new{@for="idEtiqueta"})
    @Html.EditorFor(model =>model.Id,new{htmlAttributes = new{@class="form-control",@type="numeric",@id="idEtiqueta",@value = @Model.Id,@disabled=true}} )
</div>
<div class="form-group">
    @Html.Label(null,"Nome",new{@for="nomeEtiqueta"})
    @Html.EditorFor(model =>model.Nome,new{htmlAttributes = new{@class="form-control",@type="Text",@id="nomeEtiqueta",@value = @Model.Nome,@disabled=true}} )
</div>

<div>
    @Html.ActionLink("Voltar","Index","AdminCategoria",null,new{@class="btn btn-secondary"})
</div>
