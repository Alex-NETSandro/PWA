@model PWABlog.ViewModels.ControlAcess.ControlAcessRegisterViewModel

@using (Html.BeginForm("RegisterPost", "ControlAcess",FormMethod.Post))
{
    @Html.EditorFor(model => model.NickName, new {htmlAttributes = new {@class = "form-control mr-sm-2", @type = "Text", @id = "nickName", @value = @Model.Email, @placeHolder = "Nick Name"}})
    @Html.EditorFor(model => model.Email, new {htmlAttributes = new {@class = "form-control mr-sm-2", @type = "Email", @id = "email", @value = @Model.Email, @placeHolder = "Email"}})
    @Html.EditorFor(model => model.Password, new {htmlAttributes = new {@class = "form-control mr-sm-2", @type = "Password", @id = "passwword", @value = @Model.Password, @placeHolder = "Password"}})
    @Html.EditorFor(model => model.Token, new {htmlAttributes = new {@class = "form-control mr-sm-2", @type = "Text", @id = "passwword", @value = @Model.Token,@placeHolder="Token"}})
    <button class="btn btn-primary" type="submit">Save</button>
}

@Html.ActionLink("Return","Login","ControlAcess",null,new{@class="btn btn-primary"})